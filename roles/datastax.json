{
  "name": "datastax",
  "description": "DataStax laptop",
  "json_class": "Chef::Role",
  "default_attributes": {
    "openssh": {
      "client": {
        "strict_host_key_checking": "no"
      },
      "server": {
        "print_motd": "no",
        "print_lastlog": "no",
        "x11_forwarding": "yes"
      }
    },
    "users": {
      "installed": [
        "mike",
        "datastax",
        "trading"
      ]
    },
    "java": {
      "users": {
        "datastax": "1.7",
        "trading": "1.7"
      }
    },
    "gradle": {
      "users": [
        "trading"
      ]
    },
    "ant": {
      "users": [
        "datastax"
      ]
    },
    "eclipse": {
      "users": [
        "datastax",
        "trading"
      ]
    },
    "mat": {
      "users": [
        "datastax"
      ]
    },
    "subversion": {
      "users": [
        "trading"
      ]
    },
    "chef": {
      "users": [
        "mike"
      ]
    }
  },
  "override_attributes": {
  },
  "chef_type": "role",
  "run_list": [
    "recipe[openssh]",
    "recipe[openssh::iptables]",
    "recipe[users]",
    "recipe[java]",
    "recipe[gradle]",
    "recipe[ant]",
    "recipe[eclipse]",
    "recipe[memoryanalyser]",
    "recipe[subversion]",
    "recipe[chef]",
    "recipe[packages]",
    "recipe[scripts]"
  ],
  "env_run_lists": {
  }
}
